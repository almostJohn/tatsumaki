{
	"name": "tatsumaki",
	"version": "1.0.0",
	"description": "> .",
	"scripts": {
		"build:clean": "del-cli dist",
		"build:check": "tsc",
		"build:copy-locales": "cpy locales/ dist/",
		"build": "npm run build:clean && npm run build:check && npm run build:copy-locales",
		"lint": "prettier --check . && cross-env TIMING=1 eslint src --ext ts --format=pretty",
		"format": "prettier --check . && cross-env TIMING=1 eslint src --ext ts --format=pretty",
		"fmt": "npm run format",
		"start": "env-cmd node --enable-source-maps dist/index.js",
		"start:dev": "npm run build && npm run dev",
		"dev": "env-cmd node --enable-source-maps dist/index.js",
		"deploy:commands": "env-cmd node --enable-source-maps dist/deploy.js",
		"migrate": "env-cmd ley"
	},
	"type": "module",
	"keywords": [],
	"author": "almostJohn <garcia.johngale@gmail.com>",
	"license": "AGPL-3.0",
	"dependencies": {
		"@discordjs/rest": "^2.0.1",
		"@naval-base/ms": "^3.1.0",
		"@skyra/i18next-backend": "^2.0.4",
		"bullmq": "^4.12.7",
		"common-tags": "^1.8.2",
		"dayjs": "^1.11.10",
		"diff": "^5.1.0",
		"discord-api-types": "^0.37.62",
		"discord.js": "^14.13.0",
		"env-cmd": "^10.1.0",
		"fastest-levenshtein": "^1.0.16",
		"i18next": "^23.6.0",
		"ioredis": "^5.3.2",
		"kleur": "^4.1.5",
		"nanoid": "^5.0.2",
		"pino": "^8.16.1",
		"postgres": "^3.4.2",
		"readdirp": "^3.6.0",
		"reflect-metadata": "^0.1.13",
		"tsyringe": "^4.8.0"
	},
	"devDependencies": {
		"@types/common-tags": "^1.8.3",
		"@types/diff": "^5.0.7",
		"@types/node": "^20.8.10",
		"cpy-cli": "^5.0.0",
		"cross-env": "^7.0.3",
		"del-cli": "^5.1.0",
		"eslint": "^8.52.0",
		"ley": "^0.8.1",
		"prettier": "^3.0.3",
		"type-fest": "^4.6.0",
		"typescript": "^5.2.2"
	},
	"engines": {
		"node": ">=16.0.0"
	}
