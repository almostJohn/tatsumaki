{
	"sqltools.connections": [
		{
			"previewLimit": 50,
			"server": "localhost",
			"port": 5432,
			"askForPassword": true,
			"driver": "PostgreSQL",
			"name": "tatsumaki",
			"database": "tatsumaki",
			"username": "tatsumaki"
		}
	],
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll": "explicit",
		"source.organizeImports": "never"
	},
	"i18n-ally.localesPaths": "locales",
	"i18n-ally.enabledFrameworks": ["i18next"],
	"i18n-ally.sourceLanguage": "en-US",
	"i18n-ally.displayLanguage": "en-US",
	"i18n-ally.keystyle": "nested",
	"i18n-ally.tabStyle": "tab",
	"i18n-ally.sortKeys": true,
	"i18n-ally.keepFulfilled": true,
	"i18n-ally.theme.annotation": "#79b8ff",
	"i18n-ally.theme.annotationBorder": "#c8e1ff",
	"i18n-ally.theme.annotationMissing": "#79b8ff",
	"i18n-ally.theme.annotationMissingBorder": "#f97583",
	"typescript.tsdk": "node_modules/typescript/lib"
}
